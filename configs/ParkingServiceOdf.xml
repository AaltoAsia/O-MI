<Objects xmlns="http://www.opengroup.org/xsd/odf/1.0/" >
  <Object type="ParkingService" prefix="schema http://www.schema.org/ mv http://www.schema.mobivoc.org/">
    <id>ParkingService</id>
    <InfoItem name="FindParking" method="odf" >
      <MetaData>
        <InfoItem name="InfoItemType">
          <value>odf:Method</value>
        </InfoItem>
        <InfoItem name="odf:Parameter">
          <value type="odf:Objects">
            <Objects>
              <Object type="FindParkingRequest">
                <id>Parameters</id>
                <description lang="English">List of possible parameters to request.</description>
                <InfoItem name="ParkingUsageType">
                  <value>One of mv:CarParkingSpace, mv:MotorbikeParkingSpace or mv:ElectricVehicleParkingSpace.</value>
                </InfoItem>
                <InfoItem name="ArrivalTime">
                  <value type="dateTime">yyyy-mm-ddThh:mm:ss.sss+-hh:mm</value>
                </InfoItem>
                <Object type="schema:GeoCoordinates">
                  <id>Destination</id>
                  <InfoItem name="latitude">
                    <value type="xs:double">60.187556</value>
                  </InfoItem>
                  <InfoItem name="longitude">
                    <value type="xs:double">24.8213216</value>
                  </InfoItem>
                </Object>
              </Object>
            </Objects>
          </value>
        </InfoItem>
        <InfoItem name="odf:ReturnType">
          <value type="odf:Objects">
            <Objects>
              <Object>
                <id>ParkingService</id>
                <Object type="list">
                  <id>ParkingFacilities</id>
                    <description lang="English">List of nearby parking facilities.</description>
                    <Object type="mv:ParkingFacility">
                      <id>name of parking parking facility</id>
                      <description lang="English">A parkig facility.</description>
                      <InfoItem name="Owner" type="mv:isOwnedBy"/><!-- Maybe use something from www.schema.org -->
                      <InfoItem name="MaxParkingHours"/>
                      <Object type="schema:OpeningHoursSpecification">
                        <id>openingHoursSpecification</id>
                        <InfoItem name="closes"/>
                        <InfoItem name="opens"/>
                      </Object>
                      <Object type="schema:GeoCoordinates">
                        <id>geo</id>
                        <InfoItem name="latitude"/>
                        <InfoItem name="longitude"/>
                      </Object>
                      <Object type="list">
                        <id>ParkingSpaceTypes</id>
                        <Object type="mv:ParkingUsageType">
                          <id>ParkingUsageType</id>
                          <InfoItem name="TotalCapacity" type="mv:hasTotalCapacity"/>
                          <InfoItem name="NumberOfVacantParkingSpaces" type="mv:hasNumberOfVacantParkingSpaces"/>
                          <InfoItem name="HourlyPrice" type="mv:PriceParking"/><!-- Maybe rename to Pricing? -->
                          <InfoItem name="MaxHeight" type="mv:hasVehicleHeightLimitInM"/>
                          <InfoItem name="MaxLength" type="mv:hasVehiclLengthLimitInM"/>
                          <InfoItem name="MaxWidth" type="mv:hasVehicleWidthLimitInM"/>
                          <Object type="list">
                            <id>ParkingSpaces</id>
                            <Object type="mv:ParkingSpace">
                              <id>Space</id>
                              <InfoItem name="Available"/>
                              <InfoItem name="User"/>
                            </Object>
                          </Object>
                        </Object>
                      </Object>
                    <!-- For more specific presentation of this look at following
                     ParkingLots Object.  -->
                  </Object>
                </Object>
              </Object>
            </Objects>
          </value>
        </InfoItem>
      </MetaData>
      <value>odf:Method</value>
    </InfoItem>
