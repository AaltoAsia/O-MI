      <MetaData>
        <InfoItem name="InfoItemType">
          <value>odf:Method</value>
        </InfoItem>
        <InfoItem name="odf:Parameter">
          <value type="odf:Objects">
            <Objects>
              <Object type="FindParkingRequest">
                <id>Parameters</id>
                <description lang="English">List of possible parameters to request.</description>
                <InfoItem name="ArrivalTime">
                  <value>test</value>
                </InfoItem>
                <Object type="schema:GeoCoordinates">
                  <id>Destination</id>
                  <InfoItem name="latitude">
                    <value>60.187556</value>
                  </InfoItem>
                  <InfoItem name="longitude">
                    <value>24.8213216</value>
                  </InfoItem>
                </Object>
              </Object>
            </Objects>
          </value>
        </InfoItem>
        <InfoItem name="odf:ReturnType">
          <value type="odf:Objects">
            <Objects>
              <Object>
                <id>ParkingService</id>
                <Object type="list">
                  <id>ParkingLots</id>
                  <description lang="English">List of nearby parking lots.</description>
                  <Object type="ParkingLot schema:ParkingFacility">
                    <id>name of parking lot</id>
                    <description lang="English">A parkig lot.</description>
                    <!-- For more specific presentation of this look at following
                     ParkingLots Object.  -->
                  </Object>
                </Object>
              </Object>
              </Objects>
          </value>
        </InfoItem>
      </MetaData>
      <value>odf:Method</value>
    </InfoItem>
